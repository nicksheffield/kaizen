{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"allowJs": true,
		"jsx": "preserve",
		"noEmit": true,
		"baseUrl": "src",
		"paths": {
			"mods/*": [
				"../../mods/*"
			]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
		"node_modules"
	]
}